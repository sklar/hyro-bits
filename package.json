{
  "name": "hyro-bits",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "bump:major": "npm version major -m 'Bump v%s :raised_hands:'",
    "bump:minor": "npm version minor -m 'Bump v%s :point_up:''",
    "bump:patch": "npm version patch -m 'Bump v%s'",
    "bump": "npm run bump:patch",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,mdx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx --quiet",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions and >0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=15.0"
  }
}
